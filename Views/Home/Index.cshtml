@{
    ViewBag.Title = "Home Page";
}



<div class="row">
    <div class="col-md-8">
        <h2>
        @if (User.Identity.Name!=null &&  User.Identity.Name.Length>0)
        {
            <h1>Bienvenue à @(User.Identity.Name)</h1>
        }
        else
        {
            <h1>Vous n'êtes pas identifié</h1>
        }
        </h2>
        
    </div>
    
</div>